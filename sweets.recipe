[DEFAULT]
name      = sugar-network

[Package]
project   = Server:Factory
implement = %(name)s
summary   = Client side library to access to Sugar Network server
license   = GPLv3+
homepage  = http://wiki.sugarlabs.org/go/Platform_Team/Sugar_Network/Client

version   = 0.1
stability = developer

requires  = base/requests

[Build]
install   = install -m 0755 -d %(DESTDIR)s/%(PYTHONSITEDIR)s &&
            cp -r sugar_network %(DESTDIR)s/%(PYTHONSITEDIR)s/ &&
            true
